{
  "name": "campus-house-mobile",
  "version": "1.0.0",
  "description": "캠퍼스하우스 모바일 앱 - React Native Expo 기반",
  "main": "expo-router/entry",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/campus-house/campus-house-mobile.git"
  },
  "keywords": [
    "react-native",
    "expo",
    "mobile",
    "campus",
    "housing",
    "캠퍼스하우스"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/campus-house/campus-house-mobile/issues"
  },
  "homepage": "https://github.com/campus-house/campus-house-mobile#readme",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "npx eslint . --ext .js,.ts,.tsx",
    "lint:fix": "npx eslint . --ext .js,.ts,.tsx --fix",
    "format": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "check": "npm run lint && npm run format:check",
    "type-check": "npx tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "expo": "~54.0.0",
    "expo-constants": "~18.0.9",
    "expo-font": "~14.0.8",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.7",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@eslint/js": "^9.34.0",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.0.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "glob": "^8.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.0",
    "typescript": "~5.9.2"
  },
  "overrides": {
    "glob": "^8.0.0",
    "rimraf": "^5.0.0"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "private": true
}
